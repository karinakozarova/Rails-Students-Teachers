<p id="notice"><%= notice %></p>

<h1>Karinas</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @karinas.each do |karina| %>
      <tr>
        <td><%= link_to 'Show', karina %></td>
        <td><%= link_to 'Edit', edit_karina_path(karina) %></td>
        <td><%= link_to 'Destroy', karina, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Karina', new_karina_path %>
